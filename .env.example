# Copy this file to `.env` and fill the real values. Do NOT commit `.env` to git.
# Example environment variables used by `docker-compose.yml`.

# Postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeme
POSTGRES_DB=auratech

# Backend
PORT=3333
# Compose supports interpolation; this example constructs DATABASE_URL using the values above.
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@db:5432/${POSTGRES_DB}

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:3333
